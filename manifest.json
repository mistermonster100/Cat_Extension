{
  "manifest_version": 2,
  "name": "Wandering Cat Extension",
  "version": "1.0",
  "description": "An extension that adds a wandering cat to web pages",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html"
  }
}

